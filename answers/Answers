exercise 1
SELECT *
FROM Enrollments
Left JOIN Students
ON Enrollments.id = Students.studentid;

SELECT *
FROM Enrollments
Inner Join Students
ON Enrollments.studentid=Students.studentid;

SELECT * studentid, studentname,
FROM Enrolments
Full Outer Join Students
ON Enrolments.StudentID=Students.StudentID;


